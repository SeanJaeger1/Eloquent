{
  "name": "eloquent",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build-web": "npx expo export:web",
    "lint": "eslint \"./App.tsx\" \"./components/**/*.[tj]s?(x)\" \"./hooks/**/*.[tj]s?(x)\" \"./utils/**/*.[tj]s?(x)\" --max-warnings=0",
    "lint:fix": "eslint \"./App.tsx\" \"./components/**/*.[tj]s?(x)\" \"./hooks/**/*.[tj]s?(x)\" \"./utils/**/*.[tj]s?(x)\" --fix",
    "format": "prettier --write \"./App.tsx\" \"./components/**/*.[tj]s?(x)\" \"./hooks/**/*.[tj]s?(x)\" \"./utils/**/*.[tj]s?(x)\"",
    "fix-all": "npm run format && npm run lint:fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.1.3",
    "@expo/webpack-config": "~19.0.1",
    "@react-navigation/bottom-tabs": "^6.5.14",
    "@react-navigation/native": "^6.1.12",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.8",
    "crypto-browserify": "^3.12.0",
    "dotenv": "^16.4.5",
    "expo": "~50.0.20",
    "expo-av": "~13.10.6",
    "expo-constants": "~15.4.6",
    "expo-router": "~3.4.10",
    "expo-status-bar": "~1.11.1",
    "firebase": "^10.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-reanimated": "^3.6.2",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "^14.1.0",
    "react-native-web": "~0.19.6",
    "stream-browserify": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-typescript": "^7.25.7",
    "@types/babel__core": "^7.20.5",
    "@types/react": "~18.2.45",
    "@types/react-native": "^0.72.8",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-native": "^4.1.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "tsc-files": "^1.1.4",
    "typescript": "^5.6.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "tsc-files --noEmit"
    ]
  },
  "private": true
}

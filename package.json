{
  "name": "eloquent",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:dev": "APP_ENV=development expo start",
    "start:prod": "APP_ENV=production expo start",
    "android": "expo start --android",
    "android:dev": "APP_ENV=development expo start --android",
    "android:prod": "APP_ENV=production expo start --android",
    "ios": "expo start --ios",
    "ios:dev": "APP_ENV=development expo start --ios",
    "ios:prod": "APP_ENV=production expo start --ios",
    "web": "expo start --web --bundler metro",
    "web:dev": "APP_ENV=development expo start --web --bundler metro",
    "web:prod": "APP_ENV=production expo start --web --bundler metro",
    "build:web": "APP_ENV=production expo export --platform web",
    "prebuild": "expo prebuild",
    "build:android": "APP_ENV=production eas build --platform android --profile production",
    "build:android:dev": "APP_ENV=development eas build --platform android --profile development",
    "build:ios": "APP_ENV=production eas build --platform ios --profile production",
    "build:ios:dev": "APP_ENV=development eas build --platform ios --profile development",
    "dev": "expo start --dev-client",
    "dev:dev": "APP_ENV=development expo start --dev-client",
    "dev:prod": "APP_ENV=production expo start --dev-client",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"./App.tsx\" \"./components/**/*.[tj]s?(x)\" \"./hooks/**/*.[tj]s?(x)\" \"./utils/**/*.[tj]s?(x)\" \"./services/**/*.[tj]s?(x)\" --max-warnings=0",
    "lint:fix": "eslint \"./App.tsx\" \"./components/**/*.[tj]s?(x)\" \"./hooks/**/*.[tj]s?(x)\" \"./utils/**/*.[tj]s?(x)\" \"./services/**/*.[tj]s?(x)\" --fix",
    "format": "prettier --write \"./App.tsx\" \"./components/**/*.[tj]s?(x)\" \"./hooks/**/*.[tj]s?(x)\" \"./utils/**/*.[tj]s?(x)\" \"./services/**/*.[tj]s?(x)\"",
    "fix-all": "npm run format && npm run lint:fix",
    "prepare": "husky install",
    "validate": "npm run lint && npm run typecheck && npm run test",
    "clean": "rm -rf node_modules .expo web-build && npm install"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.1",
    "@react-navigation/bottom-tabs": "^6.5.14",
    "@react-navigation/native": "^6.1.12",
    "@sentry/react-native": "~6.3.0",
    "crypto-browserify": "^3.12.0",
    "dotenv": "^16.4.5",
    "expo": "^52.0.37",
    "expo-av": "~15.0.2",
    "expo-constants": "~17.0.7",
    "expo-dev-client": "~5.0.12",
    "expo-router": "~4.0.17",
    "expo-status-bar": "~2.0.1",
    "firebase": "^10.8.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.19.13",
    "stream-browserify": "^3.0.0",
    "expo-updates": "~0.27.1",
    "expo-font": "~13.0.4",
    "expo-linking": "~7.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-typescript": "^7.25.7",
    "@testing-library/react-native": "^12.3.2",
    "@types/babel__core": "^7.20.5",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-native": "^0.72.8",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-native": "^4.1.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.5",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "react-test-renderer": "18.2.0",
    "tsc-files": "^1.1.4",
    "typescript": "^5.3.3"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "tsc-files --noEmit"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "private": true,
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
